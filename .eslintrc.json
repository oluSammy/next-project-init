{
  "extends": ["eslint:recommended", "next/core-web-vitals"],
  "globals": {
    "React": "readonly"
  },
  "rules": {
    "no-unused-vars": [
      1,
      {
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ]
  },
  "plugins": ["jest"],
  "env": {
    "jest/globals": true
  }
}
